<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javafx.properties.controller.VolumeController">
   <children>
      <Slider fx:id="volumeSlider" showTickLabels="true" snapToTicks="true" />
      <HBox>
         <children>
            <Label text="Volume:" />
            <Label fx:id="volumeLevelLabel" text=" 0" />
         </children>
      </HBox>
   </children>
</VBox>
